@{
    var context = HttpContext.Current;
    if (context.Session["type"] != null)
    {
        var s = context.Session["type"].ToString();
        if (s == "1")
        {
            Layout = "~/Views/Shared/_Layout_Admin.cshtml";
        }
        else
        {
            Layout = "~/Views/Shared/_Layout.cshtml";
        }
    }
}
